{
    "Console Log": {
        "prefix": "cll",
        "body": [
            "console.log(${1:\"$0\"});",
            "$2"
        ],
        "description": "Log output to console"
    },
    "Document Query Selector": {
        "prefix": "dqs",
        "body": [
            "document.querySelector(\"$1\")"
        ]
    },
    "Document Query Selector All": {
        "prefix": "dqsa",
        "body": [
            "document.querySelectorAll(\"$1\")"
        ]
    },
    "Document Get by ID": {
        "prefix": "dgid",
        "body": [
            "document.getElementById(\"$1\")"
        ]
    },
    "Add Event Listener": {
        "prefix": "ael",
        "body": [
            "addEventListener(${1:\"click\"}, $2 , false);"
        ]
    },
    "Add Event Listener Function": {
        "prefix": "aelf",
        "body": [
            "addEventListener(${1:\"click\"}, function () {",
            "\t$2",
            "} , false);"
        ]
    },
    "Export Arrow Function": {
        "prefix": "efn",
        "body": [
            "export const ${2:name} = (${3:params}) => {",
            "\t$0",
            "};"
        ]
    },
    "Arrow Function": {
        "prefix": "afn",
        "body": [
            "const ${2:name} = (${3:params}) => {",
            "\t$0",
            "};"
        ]
    },
    "React Import": {
        "prefix": "imr",
        "body": [
            "import React${1:, { ${2:func} \\}} from \"react\"$0"
        ]
    }
}